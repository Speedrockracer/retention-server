{
    "name": "django/RetentionAssessment",
    "description": "Assessment project",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "lodash-php/lodash-php": "^0.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {"App\\": "app/"}
    },
    "scripts": {
        "test": "phpunit --bootstrap vendor/autoload.php app --testdox"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
